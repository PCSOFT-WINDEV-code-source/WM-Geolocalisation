     name : RechercherACôtéDe
     internal_properties : CAAAAAgAAACQP1XhRp4zltqyNzlQQFdAE34vYCZIpcNAFgP1db6ktmiAXIIa3GAnrMWiDXL957mLTJjMArCj9rI2zIam9GLeHcTd8ZrkbpqTYJpVYWJ3vdzOb3LN4aSXlmnfBEcl90LKna8CTGws+1z9vr7Btr04X3Es2C1HJqFrDqiDgF9l3CkXKtO/bZxddK8UKaDfuxF4DTKOOv+aHp3eWIzHk+WhkoId6g==
     procedure_id : 1355141780517398431
     type_code : 14
     code : |1+
      // Résumé :  Effectue une recherche des magasins situés à côté de l'adresse saisie >> le rayon de recherche peut être modifié, cf constante "CST_DISTANCE" affectée à 10 kms
      // Syntaxe :
      //RechercherACôtéDe (<PositionPourRayonRecherche> est géoPosition)
      //
      // Paramètres :
      //	PositionPourRayonRecherche (géoPosition) : Position autour de laquelle on doit rechercher
      // Valeur de retour :
      // 	Aucune
      
      PROCEDURE RechercherACôtéDe(PositionPourRayonRecherche est une géoPosition)
      
      //Réinitialisation des variables utiles à la recherche
      Réinitialisation()
      
      //On effectue une recherche "à côté de", on va donc afficher la distance entre les magasins trouvés et l'adresse saisie pour la recherche
      ZR_ListeMarqueurs.LIB_Distance..Visible = Vrai	
      
      //On parcourt les magasins de la base de données
      PositionMagasin est une géoPosition
      POUR TOUT Magasin 
      	
      	//Quelle distance sépare le magasin de l'adresse saisie
      	PositionMagasin..Latitude = Magasin.Latitude
      	PositionMagasin..Longitude = Magasin.Longitude 
      	rDistance est un réel = géoDistance(PositionMagasin, PositionPourRayonRecherche)
      	
      	//Si distance inférieure à 10 kms, par exemple
      	SI rDistance < CST_DISTANCE ALORS
      		
      		sDistance est une chaine = DistanceVersChaine(rDistance)
      		
      		//On ajoute un marqueur pour ce magasin dans la carte
      		UnMarqueur est un Marqueur 
      		UnMarqueur..Position = PositionMagasin
      		UnMarqueur..ActionClic = ClicMarqueur
      		UnMarqueur..Description = Magasin.Nom + TAB + Magasin.Adresse + ESP + Magasin.Ville + TAB +  sDistance
      		CarteAjouteMarqueur(CARTE_Position, UnMarqueur)
      		
      		//On mémorise le magasin dans le tableau
      		nIndice est un entier 
      		nIndice = TableauAjoute(gtabMagasins, UnMarqueur)
      		si nIndice <0 alors
      			erreur("L'ajout dans le tableau a échoué")
      		FIN
      		//Ajout du magasin dans la zone répétée
      		ZoneRépétéeAjouteLigne(ZR_ListeMarqueurs, Magasin.Nom, Magasin.Adresse, Magasin.Ville, rDistance, sDistance, nIndice)
      		
      	FIN
      FIN
      
      //Tri de la ZR selon la distance qui nous sépare du marqueur
      ZoneRépétéeTrie("ATT_ValeurDistance")	
      
      //Indication du nombre de réponses trouvées
      LIB_NbRéponses = ChaîneConstruit("    %1 réponse(s)",ZoneRépétéeOccurrence(ZR_ListeMarqueurs))
      
      // Dé-commentez la ligne suivante pour centrer la carte sur la position autour de laquelle on recherche.
      // Dans cet exemple, on a choisi d'effectuer un affichage de la carte en tenant compte de tous les marqueurs et d'appliquer un zoom et un cadrage fonction des marqueurs.
      //CarteAffichePosition(CARTE_Position, PositionPourRayonRecherche)
      
      MaFenêtre..Plan = 2
      
     type : 458752
   -
     name : bMagasinACetteAdresse
     procedure_id : 1355212372604646126
     type_code : 14
     code : |1-
      // Résumé : Indique si le magasin est situé à l'adresse passée en paramètre
      // Syntaxe :
      //[ <Résultat> = ] bMagasinACetteAdresse (<AdresseSaisie> est Adresse)
      //
      // Paramètres :
      //	AdresseSaisie (Adresse) : Adresse de recherche
      // Valeur de retour :
      // 	booléen : Vrai si le magasin est à l'adresse saisie, Faux sinon
      //
      PROCEDURE bMagasinACetteAdresse(AdresseSaisie est une adresse)
      
      si AdresseSaisie.Ville <> "" _et_ majuscule(AdresseSaisie.Ville) = majuscule(Magasin.Ville) ALORS
      	si AdresseSaisie.Rue = "" ALORS
      		renvoyer vrai
      	sinon
      		si Majuscule(AdresseSaisie.Rue) = Majuscule(Magasin.adresse) ALORS
      			renvoyer vrai
      		FIN
      	FIN
      	
      FIN
      
      renvoyer faux
     type : 458752

   -
     name : RécupèreAdresse
     internal_properties : CAAAAAgAAAAO3YsoLGctpaQ8nBFO9Nf4RoeJi8Nl0HuwLR5IUDO0kbx+GJ9DCXcIR52W25nwCF0a+UXUWSPOtp9Jtf+zMtgrY1RTBkswaCZYAkwKrgrD9IB8CfmIOkGygAs9Y5vVK0UOGnOIW+s8/I26m5+GMPeqRW90nlWdmSImbYrdKoXofojwD+g=
     procedure_id : 1357397213875842817
     type_code : 14
     code : |1+
      // Résumé : Récupère la position GPS de l'appareil
      // Syntaxe :
      //[ <Résultat> = ] RécupèreAdresse ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	booléen : Vrai si position récupérée, Faux sinon
      
      PROCEDURE RécupèreAdresse()
      
      //Indique à l'utilisateur que la recherche de sa position est en cours
      CMOD_Position.LIB_RechercheEnCours..Visible = vrai
      
      //Affichage de l'adresse en cours
      gMaPositionEnCours = GPSRécupèrePosition(20s, "Récupération de votre position...")
      CMOD_Position.AfficheAdresse(gMaPositionEnCours)
      
      
      renvoyer vrai
      
      
      CAS ERREUR:
      CMOD_Position.LIB_RechercheEnCours..Visible = vrai
      CMOD_Position.LIB_RechercheEnCours = "Position inconnue"
      RENVOYER Faux
      
      fin :
      //Recherche terminée
      CMOD_Position.LIB_RechercheEnCours..Visible = Faux
      
     type : 458752
   -
     name : LanceRechercheACôtéDe
     procedure_id : 1880294879281224041
     type_code : 14
     code : |1-
      // Résumé : Lance la recherche 
      // Syntaxe :
      //LanceRechercheACôtéDe ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      // 	maPosition (géoPosition - valeur par défaut=0) : <indiquez ici le rôle de maPosition>
      PROCEDURE LanceRechercheACôtéDe()
      
      GR_Ailleurs..Visible = Faux
      
      // Si le suivi du déplacement avait été arrêté, on le réactive pour la recherche autour de moi
      SI gbSuiviDéplacement = Faux _ET_ CarteSuitDéplacement(CARTE_Position) = Vrai ALORS
      	gbSuiviDéplacement = Vrai 
      FIN
      
      //Si on n'avait pas encore la position en cours, on tente de la récupérer
      SI gMaPositionEnCours..Latitude = 0 ALORS
      	SI RécupèreAdresse() = Faux ALORS
      		Erreur("Votre position n'a pas pu être récupérée")
      		RETOUR
      	FIN
      FIN
      
      //Recherche les magasins autour de la position GPS actuelle
      RechercherACôtéDe(gMaPositionEnCours)
      
      
      LIB_RappelRecherche1 = TAB + "autour de moi"
      LIB_RappelRecherche2 = TAB + "autour de moi"
     type : 458752
   -
     name : DistanceVersChaine
     internal_properties : CAAAAAgAAABOplAZPHSRvDblfgv0U1oX/Ys/j//RBwyFCNVrmHz/5IaTUBliGOz5Tarkj50W7v1H1DhesBAV/MsqG1I4mNftOCUi72T4CB2/PNvuPavjLv6NglNQ7u1t1oESiQ==
     procedure_id : 1880318759305671369
     type_code : 14
     code : |1-
      // Résumé : Convertit une valeur de distance en mètres en une distance claire en langage naturel
      // Syntaxe :
      //[ <Résultat> = ] DistanceVersChaine (<rDistance>)
      //
      // Paramètres :
      //	rDistance : Distance en mètres
      // Valeur de retour :
      // 	chaîne UNICODE : Chaîne avec la distance en m, ou km ; selon la valeur
      
      PROCEDURE DistanceVersChaine(rDistance)
      
      
      sDistance est une chaîne
      SI rDistance > 999 ALORS
      	sDistance = ChaîneConstruit( "%1 km", Arrondi(rDistance /1000,1))
      SINON
      	sDistance = ChaîneConstruit( "%1 m", Arrondi(rDistance))
      FIN
      
      renvoyer sDistance
     type : 458752

   -
     name : _ChangementEtatGPS
     procedure_id : 1354835167098129566
     type_code : 14
     code : |1+
      /// Résumé : Procédure appelée à chaque modification de l'état du GPS
      // Syntaxe :
      //_ChangementEtatGPS (<nEtat>)
      //
      // Paramètres :
      //	nEtat : Nouvel état du GPS
      // Valeur de retour :
      // 	Aucune
      
      PROCEDURE _ChangementEtatGPS(nEtat)
      
      // Ignore l'appel si l'état n'a pas été modifié
      SI gnEtatActuel = nEtat ALORS
      	RETOUR
      FIN
      
      // Mémorise l'état en cours
      gnEtatActuel = nEtat
      
      
      SELON nEtat
      	CAS gpsActivé // le fournisseur a été activé par l’utilisateur.
      		RécupèreAdresse()
      		
      	CAS gpsDésactivé 	// le fournisseur a été désactivé par l’utilisateur.
      		
      	CAS gpsHorsService 	// le fournisseur est hors-service.
      		
      	CAS gpsIndisponible // le fournisseur est temporairement indisponible.
      		
      	CAS gpsDisponible	// le fournisseur est disponible.
      		RécupèreAdresse()
      FIN
      
     type : 458752
   -
     name : ClicMarqueur
     procedure_id : 1354843512221267073
     type_code : 14
     code : |1+
      // Résumé : Traitement appelé lors du clic sur un marqueur
      // Syntaxe :
      //ClicMarqueur (<MarqueurEnCours> est Marqueur)
      //
      // Paramètres :
      //	MarqueurEnCours (Marqueur) : 
      // Valeur de retour :
      // 	Aucune
      
      PROCEDURE ClicMarqueur(MarqueurEnCours est un marqueur)
      
      //Modification de l'image du marqueur cliqué
      MarqueurEnCours..image = "POI.gif"
      CarteModifieMarqueur(CARTE_Position,MarqueurEnCours)
      
      //Affichage des informations du magasin 
      LIB_NomMarqueurCarte = ExtraitChaîne(MarqueurEnCours..Description, 1, TAB)
      LIB_AdresseMarqueurCarte = ExtraitChaîne(MarqueurEnCours..Description, 2, TAB)
      LIB_DistanceMarqueurCarte = ExtraitChaîne(MarqueurEnCours..Description, 3, TAB)
      
      //Le précédent marqueur cliqué est remis dans son état initial
      SI MarqueurPrécédent <> Null ALORS
      	MarqueurPrécédent..Image = "MarqueurRepos.gif" 	
      	CarteModifieMarqueur(CARTE_Position,MarqueurPrécédent)
      FIN
      
      //Mémorisation du "futur marqueur précédent"
      MarqueurPrécédent = MarqueurEnCours
      
      

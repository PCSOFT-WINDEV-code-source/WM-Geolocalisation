        procedure_id : 1355160656903386767
        type_code : 14
        code : |1+
         // Résumé : Affiche l'adresse correspondant à la position GPS passée en paramètre
         // Syntaxe :
         //AfficheAdresse (<PositionGPS> est géoPosition)
         //
         // Paramètres :
         //	PositionGPS (géoPosition) : Position GPS
         // Valeur de retour :
         // 	Aucune
         
         PROCEDURE AfficheAdresse(PositionGPS est une geoposition)
         
         MonAdresse est une adresse
         tabAdresses est un tableau d'adresse
         SI géoRécupèreAdresse(PositionGPS..Latitude + "," + PositionGPS..Longitude, tabAdresses) = Faux ALORS
         	SI ErreurDétectée ALORS 
         		LIB_Ville = "Adresse inconnue"
         		LIB_Ville..Visible = Vrai
         		RETOUR
         	FIN
         FIN
         
         MonAdresse = tabAdresses[1]
         
         LIB_Ville = MonAdresse..Ville
         LIB_Ville..Visible = Vrai
         
         LIB_Rue = MonAdresse.Rue
         LIB_Rue..Visible = Vrai
         
        type : 458752

      -
        code : |1-
         
         selon INT_ACôtéDe
         
         	cas 0 :
         		RechercherA()
         		 
         	cas 1 :
         		
         		ClavierVisible(Faux)
         		
         		//On récupère la position GPS de l'adresse saisie
         		tabAdresses est un tableau d'adresse
         		si géoRécupèreAdresse(SAI_Ou, tabAdresses) = faux ALORS
         			si erreurdetectee alors erreur(ErreurInfo())
         			retour
         		FIN
         		
         		si tabAdresses..Occurrence = 0 ALORS
         			Erreur("Aucun magasin trouvé")
         			retour
         		FIN
         		
         		//Et on effectue la recherche des magasins autour de cette adresse
         		RechercherACôtéDe(tabAdresses[1].Position)
         		
         		LIB_RappelRecherche2 = TAB + "à côté de " + SAI_Ou
         		LIB_RappelRecherche1 = TAB + "à côté de " + SAI_Ou
         		
         		
         		
         FIN
